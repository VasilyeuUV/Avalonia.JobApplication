<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Avalonia.JobApplication.App"
             RequestedThemeVariant="Default">
  <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="avares://I3Jiad.AvaloniaUI.Controls/Themes/Generic.axaml"/>
        <ResourceInclude Source="/Controls/SimpleControls/CalendarDateTimePicker.axaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>

  <Application.Styles>
    <FluentTheme />
    <StyleInclude Source="avares://I3jiad.AvaloniaUI.Core/Icons.axaml"/>
  </Application.Styles>
</Application>
